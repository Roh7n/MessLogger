@{
}

<h1>Visitor</h1>
<div class="container">
    <table class="table table-sm table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>Contact Number</th>
                <th>Payment Date</th>
                <th>Payment Status</th>
            </tr>
        </thead>
        <tbody class="table-group-divider">
z
                @foreach (var visitor in Model)
                {
                    <tr>
                        <td>@visitor.Name</td>
                        <td>@visitor.ContactNumber</td>
                        <td>@visitor.PaymentDate</td>
                        <td>@visitor.PaymentStatus</td>
                    </tr>
                }

        </tbody>
    </table>
</div>
